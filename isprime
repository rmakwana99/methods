class isprime{
  public static boolean isPrime(int num){
  		int i;
  		boolean flag=true;
  		for(i=2;i<=num/2;i++){
  			if(num/i==0){
  				flag=false;
  				break;
  			}
  		}
  		if(flag)
  			return true;
  		else
  			return false;
  	}
  	
  public static void main(String[] args){
    System.out.println(isPrime(23));
  }
